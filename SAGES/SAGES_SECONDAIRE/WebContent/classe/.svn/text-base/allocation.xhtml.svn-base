<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
"http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"    
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:p="http://primefaces.org/ui"
      xmlns:f="http://java.sun.com/jsf/core"> 

   <ui:composition template="/WEB-INF/templates/template.xhtml">
   
    <ui:define name="meta">
   		<f:metadata>
	  		<f:viewParam name="codeclasse" value="#{classeBean.codeClasse}"/>
	  		<f:event type="preRenderView" listener="#{classeBean.initialize}"/>
		</f:metadata>
    </ui:define>
   
   <ui:define name="title">
   		SAGES SECONDAIRE | Programmation Classe
    </ui:define>
   
    <ui:define name="content">
    
	    <h2>
	   		<h:outputText value="Emploi de temps de la classe  #{classeBean.codeClasse}"/>
	   	</h2>
    	<br/>
   		<p:schedule locale="fr" view="agendaWeek" aspectRatio="0.5"/>
   		<br/>
   		<h:form>
	   		<center>
	   			<p:commandButton action="#{navBean.ressourclasselisting}" value="Retour à la liste des classes" icon=""/>
	   		</center>
	   	</h:form>
   </ui:define>
   
   </ui:composition>
 
</html>