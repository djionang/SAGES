package ages.beans.enseignant;

public class EnsClasseBean {
	private String codeenseignant;
	private String nom;
	private String matieres;
	/**
	 * @return the codeenseignant
	 */
	public String getCodeenseignant() {
		return codeenseignant;
	}
	/**
	 * @param codeenseignant the codeenseignant to set
	 */
	public void setCodeenseignant(String codeenseignant) {
		this.codeenseignant = codeenseignant;
	}
	/**
	 * @return the nom
	 */
	public String getNom() {
		return nom;
	}
	/**
	 * @param nom the nom to set
	 */
	public void setNom(String nom) {
		this.nom = nom;
	}
	/**
	 * @return the matieres
	 */
	public String getMatieres() {
		return matieres;
	}
	/**
	 * @param matieres the matieres to set
	 */
	public void setMatieres(String matieres) {
		this.matieres = matieres;
	}
	
	
}
