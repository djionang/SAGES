package ages.exception;

public class EtablissementUndefinedException extends AgesException{
	private static final long serialVersionUID = 1L;
	
	
	/**
	 * Constructeur de base, avec message d'erreur en paramètre
	 * @param msg message d'erreur
	 */
	public EtablissementUndefinedException() {
		super("Aucun etablissement trouvé ");
	}
	
	/**
	 * Constructeur avec cause d'erreur en parametre.
	 * 
	 * @param msg the error message associated with the exception
	 * @param cause the root cause of the exception
	 */
	public EtablissementUndefinedException(Throwable cause) {
		super("Aucun établissement trouvé", cause);
	}
}
