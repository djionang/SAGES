package ages.exception;

public class ChevauchementDateException extends AgesException{
	private static final long serialVersionUID = 1L;
	
	
	/**
	 * Constructeur de base, avec message d'erreur en paramètre
	 * @param msg message d'erreur
	 */
	public ChevauchementDateException(String classe) {
		super("Chevauchement "+classe);
	}
	
	/**
	 * Constructeur avec cause d'erreur en parametre.
	 * 
	 * @param msg the error message associated with the exception
	 * @param cause the root cause of the exception
	 */
	public ChevauchementDateException(String classe, Throwable cause) {
		super("Chevauchement "+classe, cause);
	}
}
